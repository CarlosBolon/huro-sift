{
\pard\plain \s51\li360\sa60\sb30\qj\widctlpar\qj\adjustright \fs20\cgrid 
\par
{
{\s17\sa60\sb30\widctlpar\qj \fs22\cgrid 
Starts the thread. }}\par
{
\pard\plain \s41\li360\widctlpar\adjustright \shading1000\cbpat8 \f2\fs16\cgrid \{\par
        // initialize attribute object\par
        if(pthread_attr_init(&threadAttribute_))\par
                throw ExceptionError("pthread_attr_init failed at");\par
\par
        // set the scheduling scope attribute\par
        if(pthread_attr_setscope(&threadAttribute_, PTHREAD_SCOPE_SYSTEM))\par
                throw ExceptionError("pthread_attr_setscope failed at");\par
\par
        if(!detached_) \par
        \{\par
                if(!runnable_.get()) \par
                \{       \par
                        if(pthread_create(&threadID_, &threadAttribute_, Thread::StartThread, (void*)this))\par
                                throw ExceptionError("pthread_create failed at");\par
                \}\par
                else \par
                \{       \par
                        if(pthread_create(&threadID_, &threadAttribute_, Thread::StartThreadRunnable, (void*)this))\par
                                throw ExceptionError("pthread_create failed at");\par
                \}\par
        \}\par
        else \par
        \{\par
                // set the detachstate attribute to detached    \par
                if(pthread_attr_setdetachstate(&threadAttribute_, PTHREAD_CREATE_DETACHED))\par
                        throw ExceptionError("pthread_attr_setdetachstate failed at");\par
\par
                if(!runnable_.get()) \par
                \{       \par
                        if(pthread_create(&threadID_, &threadAttribute_, Thread::StartThread, (void*)this))\par
                                throw ExceptionError("pthread_create failed at");\par
                \}\par
                else \par
                \{       \par
                        if(pthread_create(&threadID_, &threadAttribute_, Thread::StartThreadRunnable, (void*)this))\par
                                throw ExceptionError("pthread_create failed at");\par
                \}\par
        \}\par
\par
        if(pthread_attr_destroy(&threadAttribute_)) \par
                throw ExceptionError("pthread_attr_destroy failed at");\par
\}\par
}
}
